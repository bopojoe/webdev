{{#each bookmarks}}
  <section class="ui stacked inverted segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    
    <a href="/bookmark/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    
    <a href="/dashboard/deletebookmark/{{id}}" class="ui icon button">
      <i class="icon trash"></i>
    </a>
    
    
    <h3>Current bookmarks in this section: {{links.length}}  </h3>
  </section>
{{/each}}

